<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap-responsive.min.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.18.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.ui.autocomplete.html.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.tmpl.min.js")" type="text/javascript"></script>
    <script type="text/javascript">
        @RenderSection("ClientScripts")
        var availableTags = [
			"ActionScript",
			"AppleScript",
			"Asp",
			"BASIC",
			"C",
			"C++",
			"Clojure",
			"COBOL",
			"ColdFusion",
			"Erlang",
			"Fortran",
			"Groovy",
			"Haskell",
			"Java",
			"JavaScript",
			"Lisp",
			"Perl",
			"PHP",
			"Python",
			"Ruby",
			"Scala",
			"Scheme"
		];
        $(document).ready(function() {
            $("#search").autocomplete({
                minLength: 3,
                html: true,
                source: function (term, callback) {
                    var entries = new Array();
                    
                    var entry = new Object();
                    entry.label = $("#labelTmpl").tmpl();
                    

                    entry.value = new Object();
                    entry.value.Id = 1;
                    entries.push(entry);
                    
                    entry = new Object();
                    entry.label = $("#labelTmpl").tmpl();
                    entry.value = new Object();
                    entry.value.Id = 1;
                    entries.push(entry);
                    callback(entries);
                }
            });
        });

    </script>
    <script type="text/x-jquery-tmpl" id="labelTmpl">
        <div style="width: 700px; font-size:smaller;" class="row-fluid">
            <div class="span1">
                T
            </div>
            <div class="span5">
                marketwise mortgages ltd
            </div>
            <div class="span4">
                &nbsp;
            </div>
            <div class="span2" style="text-align: right;">
                (0113a)
            </div>
        </div>
    </script>
</head>
<body style="margin-top: 15px;">
    <div class="navbar navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <a class="brand" href="#">Project name</a>
                <form class="navbar-search pull-left" action="">
                <input id="search" type="text" class="search-query span2" placeholder="Search" />
                </form>
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse"><span
                    class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
                </a>
                <div class="nav-collapse">
                    <ul class="nav pull-right">
                        <li><a href="#">Link</a></li>
                        <li class="divider-vertical"></li>
                        <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Dropdown
                            <b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li><a href="#">Action</a></li>
                                <li><a href="#">Another action</a></li>
                                <li><a href="#">Something else here</a></li>
                                <li class="divider"></li>
                                <li><a href="#">Separated link</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <!-- /.nav-collapse -->
            </div>
        </div>
        <!-- /navbar-inner -->
    </div>
    <p>
        &nbsp;</p>
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span8">
                <div class="well">
                    @RenderBody()
                </div>
            </div>
            <div class="span4 visible-desktop hidden-phone">
                @{ Html.RenderAction("Panels", "Panel"); }
            </div>
        </div>
    </div>
</body>
</html>
